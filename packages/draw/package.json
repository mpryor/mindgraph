{
  "name": "@mindgraph/draw",
  "version": "0.6.1",
  "description": "web based graph view for markdown notes",
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "lint": "eslint src/**",
    "typecheck": "tsc --noEmit",
    "dev": "node example/server.js",
    "test": "node example/server.js & PID=$!; wait-on http://localhost:5173; cypress run --env type=actual; kill -9 $PID"
  },
  "type": "module",
  "author": "jollyjerr <jollyjerr@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/draw.umd.cjs",
  "module": "./dist/draw.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/draw.js",
      "require": "./dist/draw.umd.cjs"
    }
  },
  "devDependencies": {
    "@mindgraph/read": "workspace:*",
    "@types/d3-array": "^3.0.5",
    "@types/d3-force": "^3.0.4",
    "@types/d3-selection": "^3.0.5",
    "@types/d3-zoom": "^3.0.3",
    "express": "^4.18.2",
    "vite": "^4.3.9",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "@mindgraph/types": "workspace:*",
    "cypress": "^12.13.0",
    "cypress-visual-regression": "^3.0.0",
    "d3-array": "^3.2.3",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "wait-on": "^7.0.1"
  }
}
